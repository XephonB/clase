
git clone git@github.com:Xeo/asd.git
git remote add origin git@github.com:Xeo/asd.git
git pull origin master
git init
git status
git branch codigo
git branch -v
git checkout codigo
touch nota
git add nota
git commit -m 'comentario'
git push -u origin master
git diff   solo en unstaged
git diff --staged los que estan staged

